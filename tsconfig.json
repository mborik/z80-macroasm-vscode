{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es2020",
        "outDir": "out",
        "lib": [ "es2020" ],
        "sourceMap": true,
        "rootDir": ".",
        "strict": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedParameters": false
    },
    "exclude": [
        "node_modules",
        ".vscode-test"
    ]
}
